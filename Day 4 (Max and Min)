#maximum and minimum in a list
num = []  #created an array
n = int(input("Enter the length of your list : "))
print("Enter the  elements of list : ")

#add elements in a list by user
for i in range(n):
    num.append(int(input()))

print("List : ",num)

maxNum = num[0]
minNum = num[0]

#to find maximum number in list
for i in range(0,n):
    if maxNum < num[i]:
        maxNum = num[i]
        
#to find minimum number in list
for i in range(0,n):
    if minNum > num[i]:
        minNum = num[i]
        
print("Maximum number : ", maxNum)
print("Minimum number : ", minNum)
